{
  "name": "@cxlabs-de/express-silent",
  "version": "1.1.0",
  "description": "Add methods to respond to queries silently",
  "main": "index.js",
  "types": "types",
  "scripts": {
    "clean": "rm -rf lib; rm -rf types/lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [],
  "author": "Lukas Fabrykowski",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.6.3",
    "express": "^4.17.1",
    "prettier": "^2.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/express-serve-static-core": "^4.17.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cxlabs-de/express-silent.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "lib/!(*.map)",
    "types"
  ]
}
